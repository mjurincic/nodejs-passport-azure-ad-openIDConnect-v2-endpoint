{
  "name": "nodejs-passport-azure-ad-openidconnect-v2-endpoint",
  "version": "1.0.0",
  "description": "Web application in node.js with Express usind Azure Active Directory OpenID Connect",
  "main": "app.js",
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "address": "1.0.3",
    "body-parser": "1.18.2",
    "bunyan": "2.0.2",
    "connect-mongo": "2.0.1",
    "cookie-parser": "1.4.3",
    "dotenv": "5.0.1",
    "ejs": "2.5.7",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "joi": "13.1.2",
