    "mongoose": "5.0.10",
    "passport": "0.4.0",
    "passport-azure-ad": "3.0.12"
  },
  "devDependencies": {
    "prettier": "1.11.1",
    "eslint": "4.18.2",
    "eslint-plugin-jest": "21.14.0",
